{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-watch": "vue-cli-service build-watch",
    "test:e2e": "vue-cli-service test:e2e --url https://localhost:8443/",
    "test:e2e:headless-consent": "vue-cli-service test:e2e --spec \"./tests/e2e/specs/headless-consent-flow-run.js\" --url https://localhost:8443/ --headless ",
    "test:e2e:psu-consent": "vue-cli-service test:e2e --spec \"./tests/e2e/specs/psu-*.js\" --url https://localhost:8443/ --headless ",
    "test:e2e:all": "vue-cli-service test:e2e --url https://localhost:8443/ --headless ",
    "test:unit": "vue-cli-service test:unit --detectOpenHandles",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:unit:coverage": "vue-cli-service test:unit --coverage --runInBand --ci --detectOpenHandles",
    "dev": "vue-cli-service build --mode development --watch"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "brace": "^0.11.1",
    "esprima": "^4.0.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "redoc": "^2.0.0-rc.2",
    "register-service-worker": "^1.0.0",
    "urijs": "^1.19.1",
    "vue": "^2.5.17",
    "vue-feather-icons": "^4.7.1",
    "vue-functional-data-merge": "^2.0.7",
    "vue-notification-bell": "^0.8.11",
    "vue-router": "^3.0.1",
    "vue2-brace-editor": "^2.0.2",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.3.1",
    "@types/lodash": "^4.14.119",
    "@types/urijs": "^1.15.38",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-cypress": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "flush-promises": "^1.0.2",
    "jest-fetch-mock": "^2.0.1",
    "lint-staged": "^8.1.4",
    "mock-socket": "^8.0.5",
    "vue-cli-plugin-build-watch": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
