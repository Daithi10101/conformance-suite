{
    "references": {
      "OB3DOPAssertOnSuccess": {
        "expect": {
          "status-code": 201,
          "matches": [{
              "header-present": "x-fapi-interaction-id",
              "detail": "An RFC4122 UID used as a correlation id. The ASPSP 'plays back' the value given. If a value is not given the ASPSP MUST play back their own UUID."
            },
            {
              "JSON": "Data.Status",
              "Value": "AcceptedSettlementCompleted"
            }
          ]
        }
      },
      "OB3GLOAssertOnSuccess": {
        "expect": {
          "status-code": 200,
          "matches": [{
              "header-present": "x-fapi-interaction-id",
              "detail": "An RFC4122 UID used as a correlation id. The ASPSP 'plays back' the value given. If a value is not given the ASPSP MUST play back their own UUID."
            },
            {
              "custom": "areResourcesConsented"
            }
          ]
        }
      },
      "OB3ACCAssertOnSuccess": {
        "expect": {
          "status-code": 200,
          "matches": [{
            "header-present": "x-fapi-interaction-id",
            "detail": "An RFC4122 UID used as a correlation id. The ASPSP 'plays back' the value given. If a value is not given the ASPSP MUST play back their own UUID."
  
          }, {
            "custom": "areResourcesConsented"
          }, {
            "custom": "hasAllConsentedResources"
          }, {
            "custom": "hasAccountsPermissionsApplied"
          }]
        }
      },
      "OB3BENAssertOnSuccess": {
        "expect": {
          "status-code": 200,
          "matches": [{
            "header-present": "x-fapi-interaction-id",
            "detail": "An RFC4122 UID used as a correlation id. The ASPSP 'plays back' the value given. If a value is not given the ASPSP MUST play back their own UUID."
  
          }, {
            "custom": "areResourcesConsented"
          }, {
            "custom": "hasBeneficiaryPermissionsApplied"
          }]
        }
      },
      "OB3GLOAssertOn404": {
        "expect": {
          "status-code": 404,
          "detail": "Expected status code 404 (Not Found)."
        }
      },
      "OB3GLOAssertOn403": {
        "expect": {
          "status-code": 403,
          "detail": "Expected status code 403 (Forbidden)."
        }
      },
      "OB3GLOAssertOn401": {
        "expect": {
          "status-code": 401,
          "detail": "Expected status code 401 (Unauthorized)."
        }
      },
      "OB3GLOAssertOn400": {
        "expect": {
          "status-code": 400,
          "matches": [{
            "header-present": "x-fapi-interaction-id",
            "detail": "Expected a RFC4122 UID used as a correlation id."
          }]
        }
      },
      "basicAccountAccessConsent": {
        "permissions": ["ReadAccountsBasic"]
      },
      "detailaccountAccessConsent": {
        "permissions": ["ReadAccountDetail"]
      },
      "2paccountAccessConsent": {
        "permissions": ["ReadAccountsBasic", "ReadAccountsDetail"]
      },
      "OB3GLOAAssertNoToken": {
        "expect": {
          "status-code": 401,
          "matches": [{
              "header": "content-type",
              "value": "application/json; charset=utf-8",
              "detail": "Expected the correct content type application/json; charset=utf-8"
            },
            {
              "JSON": "Code",
              "Value": "UK.OBIE.Header.Missing"
            },
            {
              "JSON": "Errors.0.ErrorCode",
              "Value": "UK.OBIE.Header.Missing"
            },
            {
              "JSON": "Errors.0.Path",
              "Value": "Authorization"
            }
          ]
        }
      },
      "OB3GLOAssertFAPIPlayBack": {
        "expect": {
          "status-code": 200,
          "matches": [{
            "header": "x-fapi-interaction-id",
            "value": "$x-fapi-interaction-id",
            "detail": "Expected playback of in the x-fapi-interaction-id given."
          }]
        }
      },
      "OB3GLOAAssertConsentId": {
        "expect": {
          "matches": [{
            "type": "ConsentId",
            "JSON": "Data.ConsentId",
            "detail": "Expected a unique identification as assigned by the ASPSP to uniquely identify the consent resource."
          }]
        }
      }
  
    }
  }
  