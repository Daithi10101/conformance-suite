{
  "name": "compliance",
  "version": "0.0.1",
  "private": true,
  "repository": {},
  "license": "MIT",
  "scripts": {
    "build": "MIX_ENV=prod webpack --labeled-modules --hide-modules -d --display=normal --color --config build/webpack.prod.conf.js",
    "eslint": "eslint .",
    "watch": "MIX_ENV=dev webpack --watch-stdin --progress --colors --color --config build/webpack.dev.conf.js",
    "test": "VUE_CLI_BABEL_TARGET_NODE=true VUE_CLI_BABEL_TRANSPILE_MODULES=true jest src"
  },
  "dependencies": {
    "ant-design-vue": "^1.1.0",
    "axios": "^0.18.0",
    "brace": "^0.11.1",
    "phoenix": "^1.3.4",
    "phoenix_html": "^2.12.0",
    "vue": "^2.5.17",
    "vue-axios": "^2.1.3",
    "vue-router": "^3.0.1",
    "vue2-brace-editor": "^2.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/test-utils": "1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.8.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.6.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
